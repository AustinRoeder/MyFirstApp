            function operations(str) {
                var nums = str.match(/-?\d+/g);
                var s = 0;
                var p = 1;
                for (i = 0; i < nums.length; i++) {
                    s += Number(nums[i]);
                    p *= nums[i];
                }                                            
                $("#sumRes").html(s);                                            
                $("#prodRes").html(p);
            }
