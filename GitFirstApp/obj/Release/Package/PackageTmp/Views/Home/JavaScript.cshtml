        function fizzbuzz() {
            var ans = "";
            for (var i = 1; i <= 100; i++) {
                if (i % 15 == 0) {
                    ans += "FizzBuzz, ";
                }
                else if (i % 3 == 0) {
                    ans += "Fizz, ";
                }
                else if (i % 5 == 0) {
                    ans += "Buzz, ";
                }
                else
                    ans += i + ", ";
            }
            $('#fizzbuzzResults').html(ans);
        }
