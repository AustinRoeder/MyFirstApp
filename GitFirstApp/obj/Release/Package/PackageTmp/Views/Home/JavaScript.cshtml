        function filterWords() {
            var f = $("#filterFile")[0];
            var r = new FileReader();
            r.onload = function (e) {
                $('#filterRes').html
                    (filterLongWords($('#filterNum').val()
                                        , r.result));
            }
            r.readAsText(f.files[0]);
        }
        function filterLongWords(num, str) {
            var words = str.toLowerCase()
                            .match(/\w[a-zA-Z'-]{0,}/gi);
            var arr = [];
            var count = 0;
            for (var n = 0; n < words.length; n++) {
                if (words[n].length > num) {
                    count++;
                    arr.push(words[n]);
                }
            }
            arr = uniq(arr).sort().join(', ');
            $('#filterDis').html(arr)
            return count + " words longer than " + num;
        }
        </pre>
