function findWord(search) {
    var f = $("#findFile")[0];
    var r = new FileReader();
    r.onload = function (e) {
        $("#findRes").html(searchWord
                           (search, r.result));
    }
    r.readAsText(f.files[0]);
}
function searchWord(search, str) {
    var words = str.toLowerCase()
                    .match(/\w[a-z-]{0,}/gi);
    var searchS = search.toString()
                        .toLowerCase();
    var count = 0;
    for (var n = 0; n < words.length; n++) {
        if (searchS == words[n])
            count++;
    }
    return search + " appears " + count + " times."
}
