    function palindrome(str) {
        var text = str.replace(/[^0-9A-Za-z]*/g, '')
                        .toLowerCase();
        var revStr = text.split('').reverse()
                            .join('');
        $('#palinRes').html(revStr == text
                        ? str + " is a palindrome."
                        : str + " is not a palindrome.");
