                                                <div class="col-xs-6">
                                                    <pre class="brush:js">
    function armstrong() {
        var armArr = new Array();
        var b, c;
        for (var i = 100; i < 1000; i++) {
            var ti = i, c = 0;
            while (ti > 0) {
                b = ti % 10;
                c = c + (b * b * b);
                ti = parseInt(ti / 10);
            }
            if (c == i) {
                armArr.push(i);
            }
        }
        return armArr;
    }
        </pre>
