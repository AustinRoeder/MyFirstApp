        function wordFreqFinder(str) {
            var words = str.toLowerCase()
                            .match(/\w[a-z]{0,}/gi);  
            var hash = {};
            for (var n in words) {            
                if (hash[words[n]] == null) {  
                    (hash[words[n]]) = 1      
                } else {
                    (hash[words[n]])++         
                }
            };
            var wordsArranged = [];
            for (var n in hash) {              
                wordsArranged.push([n, hash[n]]);
            };
            wordsArranged.sort(function(a, b) { 
                return b[1] - a[1]
            });
            var result = "Freq List: ";  
            for (var n in wordsArranged) {
                result = result + "\n" + wordsArranged[n];
            };
            return result;
        }
