                                                    </form>
                                                </div>
                                                <div class="col-xs-6">
                                                    <pre class="brush:js">
    function happyNums(n) {
        var m, digit;
        var cycle = [];
        while (n != 1 &&
                    cycle[n] !== true) {
            cycle[n] = true;
            m = 0;
            while (n > 0) {
                digit = n % 10;
                m += digit * digit;
                n = (n - digit) / 10;
            }
            n = m;
        }
        return (n == 1);
    }
        </pre>
